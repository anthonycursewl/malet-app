{
  "expo": {
    "name": "Malet",
    "slug": "malet-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/malet/malet_app_icon.png",
    "scheme": "maletapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/malet/malet_app_icon.png",
        "backgroundColor": "#f4f4f5"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.cursewlx.maletapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/malet/malet_app_icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/malet/malet_app_icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#f4f4f5"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "expo-font",
      "expo-web-browser",
      "@react-native-google-signin/google-signin",
      "expo-secure-store",
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5eb14ff7-39b0-491a-82e4-d31e26760d14"
      }
    },
    "owner": "cursewlx"
  }
}
